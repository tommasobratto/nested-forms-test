<% if @organisations.any? %>
  <% @organisations.each do |org| %>
  <div class="org">
    <%= org.name %>
    <%= link_to org.name, organisations_path(org) %>
  </div>
  <% end %>
  <%= link_to "Create Organisation", new_organisations_path %>
<% else %>
  NO ORGS
  <%= link_to "Create Organisation", new_organisations_path %>
<% end %>
